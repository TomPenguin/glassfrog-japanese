{
  "name": "glassfrog-japanese",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon_32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.glassfrog.com/organizations/*/orgnav/roles/*/overview"
      ],
      "js": ["sideBar.js", "appHeader.js", "tabNavigation.js", "overview.js"]
    }
  ],
  "permissions": ["storage"]
}
